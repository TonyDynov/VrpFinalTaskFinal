    <validationRules>
        <fullName>EndDateValidation</fullName>
        <active>true</active>
        <description>check if reservation end date is at least 1 day later than  reservation start date</description>
        <errorConditionFormula>EndDate__c &lt;=  StartDate__c</errorConditionFormula>
        <errorDisplayField>EndDate__c</errorDisplayField>
        <errorMessage>End date cannot be set earlier than the day after start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDateValidation</fullName>
        <active>true</active>
        <description>validate that start date is not earlier than today</description>
        <errorConditionFormula>StartDate__c &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>StartDate__c</errorDisplayField>
        <errorMessage>Start day must be today or later</errorMessage>
    </validationRules>
